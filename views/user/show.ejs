<div class='container'>
	<h2>Informacion Usuario</h2>
	<br/>
	<ul>
		<li><%=user.name%></li>
		<li><%=user.lastname%></li>
		<li><%=user.username%></li>
		<li><%=user.email%></li>
	</ul>
	<br/>

	<form action="/user/edit/<%=user.id%>" method="POST" accept-charset="utf-8">
		<button type="submit" class="btn btn-success">Editar</button>
	</form>

</div>